{
  "name": "teambeat",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate:sqlite": "drizzle-kit generate --config=drizzle.config.sqlite.ts",
    "db:generate:postgres": "drizzle-kit generate --config=drizzle.config.postgres.ts",
    "db:migrate:sqlite": "drizzle-kit migrate --config=drizzle.config.sqlite.ts",
    "db:migrate:postgres": "drizzle-kit migrate --config=drizzle.config.postgres.ts",
    "db:push:sqlite": "drizzle-kit push --config=drizzle.config.sqlite.ts",
    "db:push:postgres": "drizzle-kit push --config=drizzle.config.postgres.ts",
    "db:studio": "drizzle-kit studio",
    "ngrok": "ngrok http 5173",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.35.0",
    "eslint-plugin-svelte": "^3.12.1",
    "eventsource": "^4.0.0",
    "globals": "^16.3.0",
    "less": "^4.4.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "@dicebear/collection": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@simplewebauthn/browser": "^13.1.2",
    "@simplewebauthn/server": "^13.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "cooltipz-css": "^2.4.0",
    "drizzle-orm": "^0.44.5",
    "flatpickr": "^4.6.13",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "overrides": {
    "cookie": "0.7.0"
  }
}
